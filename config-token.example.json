{
  "version": "v0.0.1-DEV_EDITION_EXPECT_CHANGES",
  "proxy": {
    "baseURL": "http://localhost:8080",
    "addr": ":8080",
    "name": "mcp-front-dev"
  },
  "mcpServers": {
    "postgres": {
      "transportType": "stdio",
      "command": "docker",
      "args": [
        "run", "--rm", "-i", "--network", "host",
        "-e", "POSTGRES_HOST=localhost",
        "-e", "POSTGRES_PORT=5432",
        "-e", "POSTGRES_DATABASE=testdb",
        "-e", "POSTGRES_USER=testuser",
        "-e", "POSTGRES_PASSWORD=testpass",
        "us-central1-docker.pkg.dev/database-toolbox/toolbox/toolbox:latest",
        "--stdio", "--prebuilt", "postgres"
      ],
      "serviceAuths": [
        {
          "type": "bearer",
          "tokens": ["dev-token-postgres-1", "dev-token-postgres-2"]
        }
      ]
    },
    "notion": {
      "transportType": "stdio",
      "command": "docker",
      "args": ["run", "--rm", "-i", "-e", "NOTION_TOKEN", "mcp/notion"],
      "env": {
        "NOTION_TOKEN": "test-notion-token"
      },
      "serviceAuths": [
        {
          "type": "bearer",
          "tokens": ["dev-token-notion-1"]
        }
      ]
    },
    "git": {
      "transportType": "stdio",
      "command": "docker",
      "args": [
        "run", "--rm", "-i",
        "-v", "/tmp/test-repos:/repos:ro",
        "mcp/git"
      ],
      "serviceAuths": [
        {
          "type": "bearer",
          "tokens": ["dev-token-git-1", "dev-token-git-2", "dev-token-git-3"]
        }
      ]
    }
  }
}